{
  "name": "react-autocomplete",
  "version": "1.2.1",
  "description": "Accessible, extensible, Autocomplete for React.js",
  "main": "./build/Autocomplete.js",
  "files": [
    "build/Autocomplete.js",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/reactjs/react-autocomplete.git"
  },
  "homepage": "https://github.com/reactjs/react-autocomplete",
  "bugs": "https://github.com/reactjs/react-autocomplete/issues",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "gh-pages": "scripty",
    "release": "scripty",
    "build": "scripty",
    "build:component": "scripty",
    "build:examples": "scripty",
    "test": "scripty",
    "test:lint": "scripty",
    "test:jest": "scripty",
    "coverage": "scripty",
    "start": "SCRIPTY_PARALLEL=true scripty"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "authors": [
    "Ryan Florence <rpflorence@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "6.1.2",
    "babel-jest": "^13.0.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.1",
    "enzyme": "^2.0.0",
    "eslint": "^2.13.1",
    "eslint-config-rackt": "^1.1.1",
    "exorcist": "^0.4.0",
    "jest": "^13.0.0",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "scripty": "^1.6.0",
    "st": "^1.2.0",
    "uglify-js": "^2.7.0",
    "watchify": "^3.7.0"
  },
  "tags": [
    "react",
    "autocomplete",
    "combobox",
    "a11y"
  ],
  "keywords": [],
  "dependencies": {
    "dom-scroll-into-view": "1.0.1"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0-0",
    "react-dom": "^0.14.7 || ^15.0.0-0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/dist/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/dist/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/enzyme"
    ]
  }
}
